config_version=5

[application]

config/name="Youtube 2D Platformer"
run/main_scene="res://scenes/ui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

