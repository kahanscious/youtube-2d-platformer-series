SceneTransition="*res://scenes/levels/scene_transition.tscn"
DeathMenu="*res://scenes/ui/death_menu/death_menu.tscn"
XPManager="*res://_globals/xp_manager.gd"
CoinManager="*res://_globals/coin_manager.gd"
CoinCounter="*res://scenes/ui/coins/coin_counter.tscn"
SettingsManager="*res://_globals/settings_manager.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=1440
window/size/window_height_override=810
window/stretch/mode="viewport"

